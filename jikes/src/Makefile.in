exec_prefix	= @exec_prefix@


#
# You should not need to edit below here
#


all:  $(EXECUTABLE)

clean:
	$(ERASE) $(EXECUTABLE)
	$(ERASE) $(OBJECTS)
	$(ERASE) core
	$(ERASE) gencode.class

distclean: clean
	$(ERASE) config.cache config.log config.status
	$(ERASE) Makefile TAGS
	$(ERASE) *~

install: all
	mkdir -p $(prefix)/bin
	strip $(EXECUTABLE)
	cp $(EXECUTABLE) $(prefix)/bin
	mkdir -p $(prefix)/man/man1
	cp ../jikes.1 $(prefix)/man/man1

